{
  "name": "Theme_Oscurin",
  "type": "dark",
  "colors": {
    "editor.background": "#000000",
    "sideBar.background": "#000000",
    "sideBar.foreground": "#ffffff",
    "editor.selectionBackground": "#ffffff1e",
    "editor.lineHighlightBackground": "#1a1a1a",
    "editorCursor.foreground": "#ffffffae",
    "editorWhitespace.foreground": "#3b3a32",

    "panel.background": "#000000",
    "activityBar.background": "#000000d7",
    "titleBar.activeBackground": "#000000",
    "statusBar.background": "#000000d7",
    "editorGutter.background": "#000000",
    "editorGroupHeader.tabsBackground": "#000000",
    "tab.activeBackground": "#0000002b",
    "tab.inactiveBackground": "#030303",

    "sideBar.border": "#ffffff20",
    "panel.border": "#ffffff3d",
    "activityBar.border": "#ffffff3d",
    "titleBar.border": "#ffffff3d",
    "statusBar.border": "#ffffff3d",
    "editorGroup.border": "#ffffff3d",
    "editorGroupHeader.tabsBorder": "#ffffff3d"
  },
  
  "tokenColors": [
      {
        "scope": "emphasis",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "strong",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "meta.diff.header",
        "settings": {
          "foreground": "#000080"
        }
      },
      {
        "scope": "comment",
        "settings": {
          "foreground": "#51ff00b2",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#008cff"
        }
      },
      {
        "scope": [
          "constant.numeric",
          "constant.other.color.rgb-value",
          "constant.other.rgb-value",
          "support.constant.color"
        ],
        "settings": {
          "foreground": "#a4da88"
        }
      },
      {
        "scope": "constant.regexp",
        "settings": {
          "foreground": "#ff25a8"
        }
      },
      {
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#7982ff"
        }
      },
      {
        "scope": "entity.name.tag.css",
        "settings": {
          "foreground": "#d7ba7d"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#2eb1f7"
        }
      },
      {
        "scope": [
          "source.css entity.other.attribute-name",
          "source.css.less entity.other.attribute-name.id",
          "source.scss entity.other.attribute-name"
        ],
        "settings": {
          "foreground": "#ffd000"
        }
      },
      {
        "scope": "invalid",
        "settings": {
          "foreground": "#ff0000"
        }
      },
      {
        "scope": "markup.underline",
        "settings": {
          "fontStyle": "underline"
        }
      },
      {
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.heading",
        "settings": {
          "foreground": "#6796e6"
        }
      },
      {
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "markup.inserted",
        "settings": {
          "foreground": "#59ff00"
        }
      },
      {
        "scope": "markup.deleted",
        "settings": {
          "foreground": "#f75050"
        }
      },
      {
        "scope": "markup.changed",
        "settings": {
          "foreground": "#569cd6"
        }
      },
      {
        "scope": "meta.selector",
        "settings": {
          "foreground": "#ffd000"
        }
      },
      {
        "name": "brackets of XML/HTML tags",
        "scope": [
          "punctuation.definition.tag"
        ],
        "settings": {
          "foreground": "#b1b1b1"
        }
      },
      {
        "scope": "meta.preprocessor",
        "settings": {
          "foreground": "#569cd6"
        }
      },
      {
        "scope": "meta.preprocessor.string",
        "settings": {
          "foreground": "#ff4800"
        }
      },
      {
        "scope": "meta.preprocessor.numeric",
        "settings": {
          "foreground": "#b5cea8"
        }
      },
      {
        "scope": "meta.structure.dictionary.key.python",
        "settings": {
          "foreground": "#9cdcfe"
        }
      },
      {
        "scope": "storage",
        "settings": {
          "foreground": "#569cd6"
        }
      },
      {
        "scope": "storage.type",
        "settings": {
          "foreground": "#008cff"
        }
      },
      {
        "scope": "storage.modifier",
        "settings": {
          "foreground": "#569cd6"
        }
      },
      {
        "scope": "string",
        "settings": {
          "foreground": "#ce7878"
        }
      },
      {
        "scope": "string.tag",
        "settings": {
          "foreground": "#ce9178"
        }
      },
      {
        "scope": "string.value",
        "settings": {
          "foreground": "#ce9178"
        }
      },
      {
        "scope": "string.regexp",
        "settings": {
          "foreground": "#f200ff"
        }
      },
      {
        "name": "JavaScript string interpolation ${}",
        "scope": [
          "punctuation.definition.template-expression.begin.js",
          "punctuation.definition.template-expression.begin.ts",
          "punctuation.definition.template-expression.end.ts",
          "punctuation.definition.template-expression.end.js"
        ],
        "settings": {
          "foreground": "#569cd6"
        }
      },
      {
        "scope": [
          "support.type.vendored.property-name",
          "support.type.property-name",
          "variable.css",
          "variable.scss",
          "variable.other.less"
        ],
        "settings": {
          "foreground": "#007bff"
        }
      },
      {
        "scope": "keyword",
        "settings": {
          "foreground": "#569cd6"
        }
      },
      {
        "scope": "keyword.control",
        "settings": {
          "foreground": "#008cff"
        }
      },
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#c18bff"
        }
      },
      {
        "scope": [
          "keyword.operator.new",
          "keyword.operator.expression",
          "keyword.operator.cast",
          "keyword.operator.sizeof",
          "keyword.operator.logical.python"
        ],
        "settings": {
          "foreground": "#58b3fe"
        }
      },
      {
        "scope": "keyword.other.unit",
        "settings": {
          "foreground": "#c3ffa3"
        }
      },
      {
        "scope": "support.function.git-rebase",
        "settings": {
          "foreground": "#ff6600"
        }
      },
      {
        "scope": "constant.sha.git-rebase",
        "settings": {
          "foreground": "#b5cea8"
        }
      },
      {
        "name": "coloring of the Java import and package identifiers",
        "scope": [
          "storage.modifier.import.java",
          "variable.language.wildcard.java",
          "storage.modifier.package.java"
        ],
        "settings": {
          "foreground": "#d4d4d4"
        }
      },
      {
        "name": "coloring of the TS this",
        "scope": "variable.language.this",
        "settings": {
          "foreground": "#569cd6"
        }
      },
      {
        "name": "Function declarations",
        "scope": [
          "entity.name.function",
          "support.function",
          "support.constant.handlebars"
        ],
        "settings": {
          "foreground": "#ffbf00"
        }
      },
      {
        "name": "Types declaration and references",
        "scope": [
          "meta.return-type",
          "support.class",
          "support.type",
          "entity.name.type",
          "entity.name.class",
          "source.cs storage.type"
        ],
        "settings": {
          "foreground": "#4EC9B0"
        }
      },
      {
        "name": "Types declaration and references, TS grammar specific",
        "scope": [
          "meta.type.cast.expr",
          "meta.type.new.expr",
          "support.constant.math",
          "support.constant.dom",
          "support.constant.json",
          "entity.other.inherited-class"
        ],
        "settings": {
          "foreground": "#4EC9B0"
        }
      },
      {
        "name": "Control flow keywords",
        "scope": "keyword.control",
        "settings": {
          "foreground": "#ff00eac3"
        }
      },
      {
        "name": "Variable and parameter name",
        "scope": [
          "variable",
          "meta.definition.variable.name",
          "support.variable"
        ],
        "settings": {
          "foreground": "#62bdee"
        }
      },
      {
        "name": "Object keys, TS grammar specific",
        "scope": [
          "meta.object-literal.key",
          "meta.object-literal.key entity.name.function"
        ],
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "name": "CSS property value",
        "scope": [
          "support.constant.property-value",
          "support.constant.font-name",
          "support.constant.media-type",
          "support.constant.media",
          "constant.other.color.rgb-value",
          "constant.other.rgb-value",
          "support.constant.color"
        ],
        "settings": {
          "foreground": "#CE9178"
        }



      }],
    "editor.tokenColorCustomizations": {
      "textMateRules": [
        {
          "scope": [
             "variable",
             "variable.other.readwrite",
            "variable.other"
          ],
          "settings": {
            "foreground": "#ff4800",
            "fontStyle": "bold"
          }}]
  }}